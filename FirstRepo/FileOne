public class MissingNumber {

	public static void main(String[] args) {

		int a[]= {1,2,4,5,8,9};
		int l=a.length;
		 int diff = a[0];
		 
		for(int i=0;i<=l-1;i++)
	    {
										// Check if diff and arr[i]-i
											// both are equal or not
	        if (a[i] - i != diff)   
	        {
	        							// Loop for consecutive
	            							// missing elements
	            while (diff < a[i] - i) 
	            {
	                System.out.print((i + diff) + " ");
	                diff++;
	            }
	        }
	    }
